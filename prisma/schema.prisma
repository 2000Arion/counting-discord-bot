// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CurrentCount {
  id       String @id
  number   Int    @default(0)
  senderId String
  targert  Int
  mode     String
}

enum gameModes {
  positive_odd
  positive_even
  negative
}
